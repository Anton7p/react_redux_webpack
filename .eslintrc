{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended"],
  "rules": {
    "no-console": ["off"],
    "no-unused-vars": "warn"
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "amd": true
  },
  "globals": {
    "$": true,
    "require": true,
    "process": true
  },
  "root": true
}